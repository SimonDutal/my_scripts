#!/usr/bin/env python3

from random import randint
from time import sleep

On_Black='\033[40m'

def question():
    q = randint(1, 80)
    if ((q % 10) == 1) or (q % 10 == 3 and q != 3):
        return "Quel est ton problème ?\n"
    if ((q % 10) == 2):
        return "Qu'est ce qui ne va pas ?\n"
    if (q == 3):
        return "t'as joué au jeu de le pouler ?\n"
    if ((q % 10) == 4):
        return "quel est le souci ?\n"
    if ((q % 10) == 5):
        return "Qu'est ce qui se passe ?\n"
    if ((q % 10) == 6):
        return "Y a un souci ?\n"
    if ((q % 10) == 7):
        return "Y a un problème ?\n"
    if ((q % 10) == 8):
        return "Y a quelque chose qui va pas ?\n"
    else:
        return "va y donne moi ton problème, j'y vais dans 5 minutes"

chaine = str(input(question()))
if len(chaine) == 0 or chaine[0] != "est-ce que je suis mort ?":
    nbr = randint(1, 6)
    if nbr == 1:
        print("regarde le man")
    if nbr == 2:
        print("lis la consigne")
    if nbr == 3:
        print("essaie avec valgrind")
    if nbr == 4:
        print("fais avec GDB")
    if nbr == 5:
        print("t'as pas free ton malloc")
    if nbr == 6:
        print("t'en as parlé à ton canard ?")
else:
    if chaine[0] == "est-ce que je suis mort ?":
        print("oui")
sleep(3)

print(On_Black + """


