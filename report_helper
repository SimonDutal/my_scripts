#!/usr/bin/bash 
file="coding-style-reports.log"
echo "making coding-style-reports.log actualy readable..."
sed -i 's/O1/O1 - Contents of the repository/g' "$file"
sed -i 's/O2/O2 - File extension/g' "$file"
sed -i 's/O3/O2 - File coherence - too many functions/g' "$file"
sed -i 's/O4/O4 - Naming files and folders/g' "$file" 
sed -i 's/G1/G1 - File header/g' "$file"
sed -i 's/G2/G2 - Separation of functions/g' "$file"
sed -i 's/G3/G3 - Indentation of preprocessor directives/g' "$file"
sed -i 's/G4/G4 - Global variables/g' "$file"
sed -i 's/G5/G5 - Include/g' "$file"
sed -i 's/G6/G6 - Line endings/g' "$file"
sed -i 's/G7/G7 - Trailing spaces/g' "$file"
sed -i 's/G8/G8 - Leading or trailing lines/g' "$file"
sed -i 's/G9/G9 - Constant values/g' "$file"
sed -i 's/G10/G10 - Inline assembly/g' "$file"
sed -i 's/F1/F1 - Coherence of functions/g' "$file"
sed -i 's/F2/F2 - Naming functions/g' "$file"
sed -i 's/F3/F3 - Number of columns/g' "$file"
sed -i 's/F4/F4 - Number of lines/g' "$file"
sed -i 's/F5/F5 - Number of parameters/g' "$file"
sed -i 's/F6/F6 - Functions without parameters/g' "$file"
sed -i 's/F7/F7 - Structures as parameters/g' "$file"
sed -i 's/F8/F8 - Comments inside a function/g' "$file"
sed -i 's/F9/F9 - Nested functions/g' "$file"
sed -i 's/L1/L1 - Code line content/g' "$file"
sed -i 's/L2/L2 - Indentation/g' "$file"
sed -i 's/L3/L3 - Spaces/g' "$file"
sed -i 's/L4/L4 - Curly brackets/g' "$file"
sed -i 's/L5/L5 - Variable declarations/g' "$file"
sed -i 's/L6/L6 - Blank lines/g' "$file"
sed -i 's/V1/V1 - Naming identifiers/g' "$file"
sed -i 's/V2/V2 - Structures/g' "$file"
sed -i 's/V3/V3 - Pointers/g' "$file"
sed -i 's/C1/C1 - Conditional branching/g' "$file"
sed -i 's/C2/C2 - Ternary operators/g' "$file"
sed -i 's/C3/C3 - Goto/g' "$file"
sed -i 's/H1/H1 - Content/g' "$file"
sed -i 's/H2/H2 - Include guard/g' "$file"
sed -i 's/H3/H3 - Macros/g' "$file"
sed -i 's/A1/A1 - Constant pointers/g' "$file"
sed -i 's/A2/A2 - Typing/g' "$file"
sed -i 's/A3/A3 - Line break at the end of file/g' "$file"
